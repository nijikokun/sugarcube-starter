{
  "name": "sugarcube-starter",
  "version": "1.0.0",
  "description": "Modern Starter kit for SugarCube",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "node .build/build_run.js",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "dev": "cross-env NODE_ENV=development node .build/build_watch.js",
    "dev:prod": "cross-env NODE_ENV=production node .build/build_watch.js",
    "start": "npm run dev"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "minify"
    ]
  },
  "keywords": [
    "twine",
    "sugarcube",
    "starter-kit",
    "boilerplate",
    "tweego"
  ],
  "author": "Nijiko Yonskai",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "babel-loader": "^8.2.5",
    "babel-preset-minify": "^0.5.1",
    "chokidar": "^3.5.3",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "css-loader": "^5.2.6",
    "debounce": "^1.2.1",
    "decompress": "^4.2.1",
    "del": "^6.0.0",
    "got": "^12.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "mvdir": "^1.0.21",
    "open": "^8.2.1",
    "postcss": "^8.4.13",
    "postcss-csso": "^6.0.0",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.5.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "slive-server": "^1.0.4",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-concat-files-plugin": "^0.5.2"
  }
}
