{
  "name": "sugarcube-starter",
  "version": "1.0.1",
  "description": "Modern Starter kit for SugarCube",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "node .build/build_run.js",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "dev": "cross-env NODE_ENV=development node .build/build_watch.js",
    "dev:prod": "cross-env NODE_ENV=production node .build/build_watch.js",
    "start": "npm run dev"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "minify"
    ]
  },
  "keywords": [
    "twine",
    "sugarcube",
    "starter-kit",
    "boilerplate",
    "tweego"
  ],
  "author": "Nijiko Yonskai",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@babel/preset-env": "^7.24.5",
    "babel-loader": "^9.1.3",
    "babel-preset-minify": "^0.5.2",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "css-loader": "^7.1.1",
    "debounce": "^2.0.0",
    "decompress": "^4.2.1",
    "del": "^7.1.0",
    "got": "^14.2.1",
    "mini-css-extract-plugin": "^2.9.0",
    "mvdir": "^1.0.21",
    "open": "^10.1.0",
    "ora": "^8.0.1",
    "postcss": "^8.4.38",
    "postcss-csso": "^6.0.1",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^9.5.12",
    "sass": "^1.77.1",
    "sass-loader": "^14.2.1",
    "slive-server": "^1.0.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-concat-files-plugin": "^0.5.2"
  }
}
